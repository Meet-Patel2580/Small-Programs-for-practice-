public class Circle{

double x;
double y;
double r;


//a
public double area(){
  double area= Math.PI*Math.pow(r,2);
  return area;
}
//b
public Circle smaller(Circle other){
  if(this.r<other.r){
    return this;
  }
  else if(this.r>other.r){
    return other;
  }
  else{
    return this;
  }
}

}