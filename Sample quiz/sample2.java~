public class sample2{
  public static void main (String[]args){
  
    int [] multiples= {1,4,6,9,12,44,55,66,23,43,5};
    lastMultiple(multiples);
  }
  public static int lastMultiple(int[] list){
    int numMultiples=0;
    int index;
      for(int i=list.length-1; i>=0;i--){
      if(list[i]%3==0){
      return i;
      
      }
    }
    
  }
}